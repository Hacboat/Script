local kavoUi = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()

local window = kavoUi.CreateLib("LightDavid v0.7","LightTheme")

 

---Tabs

 

local Tab1 = window:NewTab("Main")

local Tab1Section = Tab1:NewSection("School simulator")

local Tab2 = window:NewTab("Setting")

17loacl Tab2Section = Tab2:NewSection("Setting")

---Buttons

Tab1Section:NewButton("SS Gamepass" , "Gamepass", function()

local mt = getrawmetatable(game);

local old = mt.__namecall

local readonly = setreadonly or make_writeable

local MarketplaceService = game:GetService("MarketplaceService");

readonly(mt, false);

mt.__namecall = function(self, ...)

  local args = {...}

  local method = table.remove(args)

  if (self == MarketplaceService and method:find("UserOwnsGamePassAsync")) then

      return true

  end

  return old(self, ...)

end

end)

